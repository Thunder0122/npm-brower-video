{
  "name": "nextjs-movie-browser",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "repository": "https://github.com/topheman/nextjs-movie-browser",
  "scripts": {
    "dev": "NODE_ENV=development nodemon server",
    "build": "next build",
    "heroku-postbuild": "next build",
    "start": "NODE_ENV=production node server",
    "test": "npm-run-all --parallel --silent test:unit test:cypress",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:cypress": "npm run build && npm run cy:start-server-and-test",
    "test:cypress:dev": "npm-run-all --parallel --race dev cy:open",
    "test:cypress:debug-build": "npm run build && cross-env CYPRESS_LAUNCH_MODE=debug-build npm-run-all --parallel --race start cy:open",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:start-server-and-test": "npx start-server-and-test start :3000 cy:run",
    "ts:check": "tsc --noEmit",
    "pretty": "npx prettier --write '**/*.{tsx,ts,js,jsx,json,css,scss,md}'",
    "lint:precommit": "echo \"No precommit linting yet\" && exit 0",
    "test:precommit": "npm run ts:check && npm test"
  },
  "lint-staged": {
    "**/*.{tsx,ts,js,jsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint:precommit && npm run test:precommit"
    }
  },
  "engines": {
    "node": "^10.15.0"
  },
  "keywords": [
    "next.js",
    "React"
  ],
  "author": "Christophe Rosset <tophe@topheman.com> (http://labs.topheman.com/)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "axios-retry": "^3.1.2",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "dotenv-expand": "^4.2.0",
    "downshift": "^3.2.4",
    "express": "^4.16.4",
    "git-rev-sync": "^1.12.0",
    "hoist-non-react-statics": "^3.3.0",
    "invariant": "^2.2.4",
    "isomorphic-fetch": "^2.2.1",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "moment": "^2.24.0",
    "next": "^8.0.1",
    "next-i18next": "^0.21.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-i18next": "9.0.9"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@types/classnames": "^2.2.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/hoist-non-react-statics": "^3.0.1",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^23.3.13",
    "@types/next": "^7.0.6",
    "@types/node": "^10.12.18",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@zeit/next-typescript": "^1.1.1",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.5",
    "cypress-testing-library": "^2.3.6",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "1.16.0",
    "react-testing-library": "^5.5.3",
    "start-server-and-test": "^1.7.11",
    "typescript": "^3.2.4"
  }
}
